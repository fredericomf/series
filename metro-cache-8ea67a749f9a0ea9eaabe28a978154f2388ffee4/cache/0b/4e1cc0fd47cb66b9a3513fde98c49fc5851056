    var nameCompare = function nameCompare(a, b) {
        if (a === b) {
            return 0;
        } else if (a === MIN_NAME || b === MAX_NAME) {
            return -1;
        } else if (b === MIN_NAME || a === MAX_NAME) {
            return 1;
        } else {
            var aAsInt = tryParseInt(a),
                bAsInt = tryParseInt(b);

            if (aAsInt !== null) {
                if (bAsInt !== null) {
                    return aAsInt - bAsInt == 0 ? a.length - b.length : aAsInt - bAsInt;
                } else {
                    return -1;
                }
            } else if (bAsInt !== null) {
                return 1;
            } else {
                return a < b ? -1 : 1;
            }
        }
    };

